package TUGAS_ASD3;

public class StackFloat {
	ArrayList<Float>st=new ArrayList<Float>();
	public int ecor = -1;
	
	public void push(float value){
		eror++;
		st.add(eror,value);
	}
	public float pop(){
		float value = ecor;
		if (eror>-1){
			value = st.get(eror);
			st.remove(eror);
			ecor--;
		}
		return value;}
		public void cetak(){
	System.out.println(st.toString());
		}
}
